{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "lib": ["dom", "esnext", "es2015"],
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "build",
      "baseUrl": ".",
      "paths": {
        "*": [
          "src/*"
        ]
      }
    },
    "include": [
      "src"
    ]
  }
  
